    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "API_ENV_MODE": "dev",
                "ENV_FILE": ".env.dev"
            }
        },
        {
            "name": "Python: Current All Code",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "API_ENV_MODE": "dev",
                "ENV_FILE": ".env.dev"
            }
        }
    ]
}